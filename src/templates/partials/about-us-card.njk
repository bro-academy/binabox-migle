<ol class="about-us__list">
    {% for about in aboutUs.cards %}
          <li class="about-us__list-item" tabindex="0">
                      <p class="about-us__list-number">{{ about.number }}</p>
                      <h3 class="about-us__list-title">{{ about.title }}</h3>
                      <p class="about-us__list-desc">
                        {{ about.desc }}
                      </p>
          </li>            
    {% endfor %}
</ol>
          